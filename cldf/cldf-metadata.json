                    },
                    {
                        "datatype": "string",
                        "name": "Indonesian"
                    },
                    {
                        "datatype": "string",
                        "name": "Semantic_Field"
                    },
                    {
                        "datatype": "string",
                        "name": "Elicitation_Notes"
                    },
                    {
                        "datatype": "boolean",
                        "name": "Core_Set"
                    },
                    {
                        "datatype": {
                            "base": "string",
                            "format": "[0-9]+"
                        },
                        "name": "Concepticon_ID",
                        "valueUrl": "http://glottolog.org/resource/languoid/id/{Concepticon_ID}",
                        "required": false
                    },
                    {
                        "datatype": "string",
                        "name": "Comment"
                    }
                ],
                "primaryKey": [
                    "ID"
                ]
            },
            "url": "concepts.csv"
        },
        {
