                            "Form_ID_Target"
                        ],
                        "reference": {
                            "resource": "forms.csv",
                            "columnReference": [
                                "ID"
                            ]
                        }
                    },
                    {
                        "columnReference": [
                            "Form_ID_Source"
                        ],
                        "reference": {
                            "resource": "forms.csv",
                            "columnReference": [
                                "ID"
                            ]
                        }
                    }
                ],
                "primaryKey": [
